
@model AnimalTinder.Models.AddToRoleModel
@{
    ViewBag.Title = "AddToRole";
}

<h2>AddToRole</h2>

@using (Html.BeginForm("AddToRole", "Account", FormMethod.Post, new { enctype = "multipart/form-data" }))
{<div>
    <div class="form-group">
        
        @Html.LabelFor(model=>model.Email, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownListFor(model=>model.Email, new SelectList(@ViewBag.Users, new { @class = "form-control" }))
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model=>model.Role, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownListFor(model=>model.Role, new SelectList(@ViewBag.Roles, new { @class = "form-control" }))
        </div>

        <div class="form-group">
            <input type="submit" value="AddUserToRole"/>
        </div>
    </div>
</div>
}

